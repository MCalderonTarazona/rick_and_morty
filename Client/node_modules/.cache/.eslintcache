hname }}><div className={style.cardLink}><FontAwesomeIcon icon={faBarcode} /></div></Link>\n         \n      </div>\n   );\n}\n\n/*export function mapDispatchToProps(dispatch) {\n   return {\n      addFav: (personaje) => dispatch(addFav(personaje)),\n      removeFav: (id) => dispatch(removeFav(id))\n   }\n}\n\nexport function mapStateToProps(state) {\n   return {\n      myFavorites: state.myFavorites,\n   }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Card);*/\n",{"ruleId":"86","severity":1,"message":"87","line":64,"column":5,"nodeType":"88","endLine":64,"endColumn":13,"suggestions":"89"},{"ruleId":"86","severity":1,"message":"90","line":36,"column":4,"nodeType":"88","endLine":36,"endColumn":17,"suggestions":"91"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'navigate'. Either include it or remove the dependency array.","ArrayExpression",["92"],"React Hook useEffect has a missing dependency: 'props.id'. Either include it or remove the dependency array.",["93"],{"desc":"94","fix":"95"},{"desc":"96","fix":"97"},"Update the dependencies array to be: [access, navigate]",{"range":"98","text":"99"},"Update the dependencies array to be: [myFavorites, props.id]",{"range":"100","text":"101"},[1960,1968],"[access, navigate]",[1007,1020],"[myFavorites, props.id]"]